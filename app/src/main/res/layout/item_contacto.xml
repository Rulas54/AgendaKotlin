            android:text="Telefono:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/txtTelefono"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="12dp"
            android:layout_marginTop="17dp" app:layout_constraintTop_toBottomOf="@+id/txtDireccion"/>
